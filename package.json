{
  "name": "rock",
  "version": "1.7.0",
  "description": "Rock | Development testing",
  "repository": "https://github.com/ggallon/rock.git",
  "author": "GwenaÃ«l Gallon",
  "license": "MIT",
  "scripts": {
    "start": "meteor --settings settings-development.json",
    "visualizer": "meteor --settings settings-development.json --extra-packages bundle-visualizer --production",
    "test": "TEST_BROWSER_DRIVER=nightmare meteor test --once --driver-package meteortesting:mocha --settings settings-development.json",
    "test-app": "TEST_BROWSER_DRIVER=nightmare meteor test --full-app --once --driver-package meteortesting:mocha --settings settings-development.json",
    "test-watch": "TEST_BROWSER_DRIVER=nightmare meteor test --driver-package meteortesting:mocha --settings settings-development.json",
    "test-app-watch": "TEST_BROWSER_DRIVER=nightmare meteor test --full-app --driver-package meteortesting:mocha --settings settings-development.json",
    "test-watch-terminal": "TEST_WATCH=1 TEST_BROWSER_DRIVER=nightmare meteor test --driver-package meteortesting:mocha --settings settings-development.json",
    "test-app-watch-terminal": "TEST_WATCH=1 TEST_BROWSER_DRIVER=nightmare meteor test --full-app --driver-package meteortesting:mocha --settings settings-development.json",
    "chimp-watch": "chimp --ddp=http://localhost:3000 --watch --mocha --path=tests",
    "chimp-test": "chimp --ddp=http://localhost:3000 --mocha --path=tests",
    "staging": "meteor deploy staging.meteor.com --settings settings-development.json",
    "production": "meteor deploy production.meteor.com --settings settings-production.json"
  },
  "dependencies": {
    "@babel/runtime": "7.0.0-beta.40",
    "@ggallon/react-komposer": "2.0.6",
    "bcrypt": "1.0.3",
    "meteor-node-stubs": "0.3.2",
    "moment": "2.20.1",
    "moment-timezone": "0.5.14",
    "nightmare": "2.10.0",
    "prop-types": "15.6.0",
    "react": "15.6.2",
    "react-bootstrap": "0.32.1",
    "react-dom": "15.6.2",
    "react-loadable": "5.3.1",
    "react-router-bootstrap": "0.24.4",
    "react-router-dom": "4.2.2",
    "simpl-schema": "1.4.2",
    "sweetalert": "2.1.0",
    "uniforms": "1.23.0",
    "uniforms-bootstrap3": "1.23.0"
  },
  "devDependencies": {
    "babel-eslint": "8.2.1",
    "chai": "4.1.2",
    "chimp": "0.51.0",
    "eslint": "4.17.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-import-resolver-meteor": "0.4.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-meteor": "4.2.0",
    "eslint-plugin-react": "7.6.1"
  }
}
